{
  "name": "maxaverage",
  "version": "1.0.0",
  "description": "MaxAverage tool made by Amir Afzali",
  "main": "index.ts",
  "scripts": {
    "build": "webpack --mode production && babel src --out-dir out --extensions \".ts,.tsx\" --source-maps inline",
    "start": "npm run build && node out/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --exec babel-node --extensions \".ts,.tsx\" src/server/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Amir Afzali",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-loader": "^8.3.0",
    "concurrently": "^4.1.1",
    "css-loader": "^6.7.3",
    "eslint": "^6.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^1.19.1",
    "style-loader": "^3.3.1",
    "typescript": "^3.5.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@types/express": "^4.17.0",
    "@types/morgan": "^1.7.35",
    "@types/react": "^16.8.23",
    "@types/react-bootstrap": "^0.32.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "^8.0.0",
    "eslint-config-airbnb": "^17.1.1",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.6"
  }
}
